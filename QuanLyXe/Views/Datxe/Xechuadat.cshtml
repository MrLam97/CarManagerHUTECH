@model IEnumerable<QuanLyXe.Models.sp_Xechuadat_Result>

<table id="datatable" class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Mã xe</th>
            <th>Biển số</th>
            <th>Nhãn hiệu</th>
            <th>Tình trạng</th>
            <th>Trạng thái</th>
            <th>Loại xe</th>
            <th>Số chuyến</th>
            <th>Số KM</th>
            <th>Chọn</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.MAXE</td>
                <td>@item.BIENSO</td>
                <td>@item.NHANHIEU</td>
                <td>@item.TENTINHTRANG</td>
                <td style="color:red">@item.TENTRANGTHAI</td>
                <td>@item.TENLOAIXE</td>
                <td>@item.SOCHUYEN</td>
                <td>@item.SOKM</td>
                <td><input type="radio" class="flat" name="chon" value="@item.MAXE"></td>
            </tr>
        }
        @Html.Action("Xedadat", "Datxe")
    </tbody>
</table>
