model IEnumerable<QuanLyXe.Models.ThongtinDatxe>

    @{
        ViewBag.Title = "Lichsudatnv";
        Layout = "~/Views/Shared/_LayoutUser.cshtml";
    }

    <div class="col-md-12 col-sm-12 col-xs-12">
        <div class="x_panel">
            <div class="x_title">
                <h2>Lịch sử đặt xe</h2>
                <ul class="nav navbar-right panel_toolbox">
                    <li>
                        <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                    </li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false"><i class="fa fa-wrench"></i></a>
                        <ul class="dropdown-menu" role="menu">
                            <li>
                                <a href="#">Settings 1</a>
                            </li>
                            <li>
                                <a href="#">Settings 2</a>
                            </li>
                        </ul>
                    </li>
                    <li>
                        <a class="close-link"><i class="fa fa-close"></i></a>
                    </li>
                </ul>
                <div class="clearfix"></div>
            </div>
            <div class="x_content">
                <table id="datatable" class="table table-striped table-bordered">
                    <thead>
                        <tr>
                            <th>Ngày đặt xe</th>
                            <th>Trạng thái đặt</th>
                            <th>Loại xe</th>
                            <th>Ngày đi</th>
                            <th>Ngày về</th>
                            <th>Giờ đi</th>
                            <th>Giờ về</th>
                            <th>Lộ trình đi</th>
                            <th>Lộ trình về</th>
                            <th>Chủ trì</th>
                            <th>Số lượng</th>
                            <th>Người đề xuất</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.NGAYDX</td>
                                <td style="color:red">@item.TENTRANGTHAI</td>
                                <td>@item.TENLOAIXE</td>
                                <td>@item.NGAYDI</td>
                                <td>@item.NGAYVE</td>
                                <td>@item.GIODI</td>
                                <td>@item.GIOVE</td>
                                <td>@item.LOTRINHDI</td>
                                <td>@item.LOTRINHVE</td>
                                <td>@item.CHUTRIDOAN</td>
                                <td>@item.SOLUONGDOAN</td>
                                <td>@item.HOTEN</td>
                            </tr>
                        }
                    </tbody>
                </table>


            </div>
        </div>
    </div>



